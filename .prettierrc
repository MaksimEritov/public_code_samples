{
  "arrowParens": "always",
  "bracketSpacing": true,
  "printWidth": 120,
  "proseWrap": "always",
  "quoteProps": "as-needed",
  "semi": true,
  "singleQuote": false,
  "tabWidth": 4,
  "trailingComma": "es5",
  "useTabs": false,
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": false,
  "importOrderCaseInsensitive": true
}
